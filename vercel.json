{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      },
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/users",
        "dest": "/api/index.js"
      },
      {
        "src": "/api/users/(.*)",
        "dest": "api/index.js"
      },
      {
        "src": "/api/orders",
        "dest": "/api/index.js"
      },
      {
        "src": "/api/orders/(.*)",
        "dest": "api/index.js"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }